<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="190.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="loginLabel" layoutX="50.0" layoutY="36.0" text="Log In">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="loginButton" layoutX="203.0" layoutY="131.0" mnemonicParsing="false" onMouseClicked="#authenticate" text="Log In">
         <font>
            <Font size="12.0" />
         </font>
      </Button>
      <TextField fx:id="username" layoutX="50.0" layoutY="69.0" prefWidth="200.0" promptText="Username">
         <font>
            <Font size="12.0" />
         </font>
      </TextField>
      <PasswordField fx:id="password" layoutX="50.0" layoutY="100.0" prefWidth="200.0" promptText="Password">
         <font>
            <Font size="12.0" />
         </font>
      </PasswordField>

      <Label fx:id="errorLabel" layoutX="50.0" layoutY="135.0" text="Error">
         <font>
            <Font size="12.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
